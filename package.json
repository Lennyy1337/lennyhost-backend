{
  "name": "@lennyhost/backend",
  "version": "1.0.0",
  "description": "Open source file hosting backend",
  "scripts": {
    "start": "export NODE_ENV=production && ts-node ./src/main.ts",
    "build": "rm -rf build && pnpm db:generate && tsc",
    "dev": "export NODE_ENV=development && tsx watch ./src/main.ts | pino-pretty",
    "db:push": "prisma db push",
    "db:pull": "prisma db pull",
    "db:generate": "prisma generate",
    "clean": "rm -rf node_modules build",
    "format": "prettier --write **/*"
  },
  "keywords": [],
  "author": "",
  "license": "NOT_YET",
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.2.0",
    "@fastify/rate-limit": "^9.1.0",
    "@prisma/client": "^5.14.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.27.0",
    "fastify-type-provider-zod": "^1.1.9",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^5.14.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.12",
    "pino-pretty": "^11.0.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.10.2",
    "typescript": "^5.4.5"
  }
}
